    <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
	var toggleText = document.getElementById("menu-toggle");
	if (toggleText.innerHTML == "&gt;&gt;") {
            toggleText.innerHTML = "<<";
        } else {
            toggleText.innerHTML = ">>";
        }
    });
    </script>
